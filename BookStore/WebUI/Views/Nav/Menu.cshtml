@model IEnumerable<string>

@Html.ActionLink("Home", "List", "Books", null, new { @class = "btn" })
@foreach (var link in Model)
{
    @Html.RouteLink(link, new
{
    controller = "Books",
    action ="List",
    genre = link,
    page = 1
},
    new
    {
        @class= "btn"+ (link == ViewBag.SelectedGenre ? "btn grey" : "")
    })
}
